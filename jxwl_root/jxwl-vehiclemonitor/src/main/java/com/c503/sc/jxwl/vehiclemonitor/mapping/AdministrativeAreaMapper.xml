<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.c503.sc.jxwl.vehiclemonitor.dao.IAdministrativeAreaDao" >
  <resultMap id="BaseResultMap" type="com.c503.sc.jxwl.vehiclemonitor.bean.AdministrativeAreaEntity" >
    <id column="ID" property="id" jdbcType="VARCHAR" />
    <result column="AREA_NAME" property="areaName" jdbcType="VARCHAR" />
    <result column="AREA_ID" property="areaId" jdbcType="VARCHAR" />
    <result column="POINTS" property="points" jdbcType="VARCHAR" />
  </resultMap>
  
  <sql id="Base_Column_List" >
    ID, AREA_NAME, AREA_ID, POINTS
  </sql>
  <!-- 根据ID查询终端 -->
  <select id="findById" resultMap="BaseResultMap" parameterType="java.lang.String" >
    SELECT 
	    <include refid="Base_Column_List" />
    FROM T_ADMINISTRATIVE_AREA
    WHERE ID = #{id}
  </select>
  <select id="findAll" resultMap="BaseResultMap" parameterType="java.lang.String" >
    SELECT 
	    <include refid="Base_Column_List" />
    FROM T_ADMINISTRATIVE_AREA
  </select>
 
  
</mapper>
